    "@types/jest": "^22.0.1",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^9.3.0",
    "@types/pg": "^7.4.1",
    "@types/sinon": "^4.1.3",
    "jest": "^22.1.2",
    "mock-fs": "^4.4.2",
    "prettier": "1.10.2",
    "sinon": "^4.1.6",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.6.2"
  },
